<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.shape.Arc?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="projectView" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.fxtesting.ProjectView">
    <children>
        <AnchorPane prefHeight="500.0" prefWidth="356.0" style="-fx-background-color: #0598ff;">
            <children>
                <Arc fill="WHITE" layoutX="165.0" layoutY="200.0" length="270.0" radiusX="100.0" radiusY="100.0" startAngle="45.0" stroke="WHITE" strokeType="INSIDE" type="ROUND" />
            </children></AnchorPane>
      <AnchorPane fx:id="AnchorPaneProjectView" layoutX="356.0" layoutY="1.0" prefHeight="500.0" prefWidth="350.0">
         <children>
              <Label layoutX="111.0" layoutY="33.0" prefHeight="80.0" prefWidth="182.0" text="Project View" textAlignment="JUSTIFY" textFill="#0598ff">
                  <font>
                      <Font size="25.0" />
                  </font>
            </Label>
              <Button fx:id="createProject" layoutX="118.0" layoutY="153.0" mnemonicParsing="false" onAction="#createProjectClicked" prefHeight="25.0" prefWidth="128.0" style="-fx-background-color: #0598ff;" text="Create project" textFill="WHITE" />
              <Button fx:id="seeAllProjects" layoutX="118.0" layoutY="197.0" mnemonicParsing="false" onAction="#seeAllProjectsClicked" prefHeight="25.0" prefWidth="128.0" style="-fx-background-color: #0598ff;" text="See all projects" textFill="WHITE" />
              <Button fx:id="deleteProject" layoutX="118.0" layoutY="239.0" mnemonicParsing="false" onAction="#deleteProjectClicked" prefHeight="25.0" prefWidth="128.0" style="-fx-background-color: #0598ff;" text="Delete project" textFill="WHITE" />
              <Button fx:id="signOut" layoutX="118.0" layoutY="281.0" mnemonicParsing="false" onAction="#signOutClicked" prefHeight="25.0" prefWidth="128.0" style="-fx-background-color: #0598ff;" text="Sign out" textFill="WHITE" />
         </children>
      </AnchorPane>
    </children>
</AnchorPane>
